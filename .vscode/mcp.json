      "args": [
        "-NoProfile",
        "-File",
        "${workspaceFolder}/scripts/mm2024-mcp.ps1",
        "-Workspace",
        "${workspaceFolder}"
      ],
      "env": {
        "MM2024_COM_PROGID": "SongsDB5.SDBApplication"
      },
      "dev": {
        "watch": "src/mm2024_mcp/**/*.py"
      }
    }
  },
  "inputs": []
}
